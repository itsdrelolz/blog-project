{
  "name": "blog-project",
  "private": true,
  "workspaces": [
    "apps/client",
    "apps/backend",
    "apps/shared"
  ],
  "scripts": {
    "build:client": "npm --workspace=apps/client run build",
    "build:backend": "npm --workspace=apps/backend run build",
    "prisma:generate": "npm --workspace=apps/backend run prisma:generate",
    "start:backend": "npm --workspace=apps/backend run start",
    "dev:client": "npm --workspace=apps/client run dev",
    "dev:backend": "npm --workspace=apps/backend run dev"
  },
  "devDependencies": {
    "@types/react": "^18.3.18",
    "@types/react-router-dom": "^5.3.3",
    "prettier": "3.3.3"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.0"
  }
}
